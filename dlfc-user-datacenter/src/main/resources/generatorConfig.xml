        <javaClientGenerator type="XMLMAPPER" targetPackage="com.dlfc.user.mapper" targetProject="src/main/java">
            <property name="enableSubPackages" value="true"/>
        </javaClientGenerator>

        <table tableName="cms_guestbook" schema="dlfc" domainObjectName="CmsGuestbook">
